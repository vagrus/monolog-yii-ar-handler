{
    "name": "vagrus/monolog-yii-ar-handler",
    "description": "A Monolog handler for send logs in Yii Active Record model",
    "keywords": ["log", "monolog", "yii"],
    "type": "library",
    "require": {
        "php": ">=5.3.0",
        "monolog/monolog": "~1.17.1",
        "yiisoft/yii": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5",
        "phpunit/phpunit-mock-objects": "2.3.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "vagrus",
            "email": "no@email.com"
        }
    ],
    "autoload": {
        "psr-4": {"Vagrus\\Monolog\\": "src/Vagrus/Monolog"}
    },
    "autoload-dev": {
        "psr-4": {"Vagrus\\Monolog\\": "tests/Vagrus/Monolog"}
    }
}
